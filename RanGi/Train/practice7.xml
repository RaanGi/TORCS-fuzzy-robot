<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE params SYSTEM "params.dtd">


<params name="Practice">
  <section name="Header">
    <attstr name="name" val="Practice"/>
    <attstr name="description" val="Practice"/>
    <attnum name="priority" val="100"/>
    <attstr name="menu image" val="data/img/splash-practice.png"/>
    <attstr name="run image" val="data/img/splash-run-practice.png"/>
  </section>

	<section name="Tracks">
		<attnum name="maximum number" val="20"/>
		
		<section name="1">
			<attstr name="name" val="corkscrew"/>
			<attstr name="category" val="road"/>
		</section>
		<section name="2">
			<attstr name="name" val="wheel-1"/>
			<attstr name="category" val="road"/>
		</section>
		<section name="3">
			<attstr name="name" val="brondehach"/>
			<attstr name="category" val="road"/>
		</section>
		<section name="4">
			<attstr name="name" val="e-track-3"/>
			<attstr name="category" val="road"/>
		</section>
		<section name="5">
			<attstr name="name" val="g-track-1"/>
			<attstr name="category" val="road"/>
		</section>
		<section name="6">
			<attstr name="name" val="e-track-6"/>
			<attstr name="category" val="road"/>
		</section>
		<section name="7">
			<attstr name="name" val="g-track-3"/>
			<attstr name="category" val="road"/>
		</section>
		<section name="8">
			<attstr name="name" val="street-1"/>
			<attstr name="category" val="road"/>
		</section>
		<section name="9">
			<attstr name="name" val="wheel-2"/>
			<attstr name="category" val="road"/>
		</section>
		<section name="10">
			<attstr name="name" val="ruudskogen"/>
			<attstr name="category" val="road"/>
		</section>
		<section name="11">
			<attstr name="name" val="aalborg"/>
			<attstr name="category" val="road"/>
		</section>
		<section name="12">
			<attstr name="name" val="alpine-1"/>
			<attstr name="category" val="road"/>
		</section>
		<section name="13">
			<attstr name="name" val="dirt-3"/>
			<attstr name="category" val="dirt"/>
		</section>
		<section name="14">
			<attstr name="name" val="michigan"/>
			<attstr name="category" val="oval"/>
		</section>
		<section name="15">
			<attstr name="name" val="forza"/>
			<attstr name="category" val="road"/>
		</section>
		<section name="16">
			<attstr name="name" val="alpine-2"/>
			<attstr name="category" val="road"/>
		</section>		
		<section name="17">
			<attstr name="name" val="e-track-2"/>
			<attstr name="category" val="road"/>
		</section>
    
	</section>

  <section name="Races">
    <section name="1">
      <attstr name="name" val="Practice"/>
    </section>

  </section>

  <section name="Practice">
    <attnum name="laps" val="100"/>
    <attstr name="type" val="practice"/>
    <attstr name="starting order" val="drivers list"/>
    <attstr name="restart" val="yes"/>
    <attstr name="display mode" val="normal"/>
    <attstr name="display results" val="yes"/>
    <attstr name="invalidate best lap on wall touch" val="yes"/>
    <attstr name="invalidate best lap on corner cutting" val="yes"/>
    <attstr name="corner cutting time penalty" val="yes"/>
    <attnum name="fuel consumption factor" min="0" max="5" val="1"/>
    <attnum name="damage factor" min="0" max="5" val="1"/>
    <attnum name="distance" unit="km" val="0"/>
    <section name="Starting Grid">
      <attnum name="rows" val="1"/>
      <attnum name="distance to start" val="100"/>
      <attnum name="distance between columns" val="20"/>
      <attnum name="offset within a column" val="10"/>
      <attnum name="initial speed" unit="km/h" val="0"/>
      <attnum name="initial height" unit="m" val="0.2"/>
    </section>

  </section>

  <section name="Drivers">
    <attnum name="maximum number" val="1"/>
    <attstr name="focused module" val="human"/>
    <attnum name="focused idx" val="1"/>
    <section name="1">
      <attnum name="idx" val="7"/>
      <attstr name="module" val="RanGi"/>
    </section>

  </section>

  <section name="Configuration">
    <attnum name="current configuration" val="4"/>
    <section name="1">
      <attstr name="type" val="track select"/>
    </section>

    <section name="2">
      <attstr name="type" val="drivers select"/>
    </section>

    <section name="3">
      <attstr name="type" val="race config"/>
      <attstr name="race" val="Practice"/>
      <section name="Options">
        <section name="1">
          <attstr name="type" val="race length"/>
        </section>

        <section name="2">
          <attstr name="type" val="display mode"/>
        </section>

      </section>

    </section>

  </section>

  <section name="Drivers Start List">
    <section name="1">
      <attstr name="module" val="RanGi"/>
      <attnum name="idx" val="7"/>
    </section>

  </section>

</params>
